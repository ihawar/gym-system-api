{
  "name": "gym-system-api",
  "version": "1.0.0",
  "description": "A gym management system's API. Developed with expressjs.",
  "homepage": "https://github.com/ihawar/gym-system-api#readme",
  "bugs": {
    "url": "https://github.com/ihawar/gym-system-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihawar/gym-system-api.git"
  },
  "license": "ISC",
  "author": "ihawar",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch src --exec tsx src/server.ts",
    "build": "rimraf dist && tsup src/server.ts --format esm --dts",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "jose": "^6.1.3",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@types/pino-http": "^5.8.4",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
